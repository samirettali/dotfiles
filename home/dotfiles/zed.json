{
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_features": {
    "calt": false
  },
  "buffer_font_size": 14,
  "buffer_font_weight": 600,
  "cursor_blink": false,
  "inlay_hints": {
    "enabled": false
  },
  "journal": {
    "hour_format": "hour24"
  },
  "outline_panel": {
    "dock": "right"
  },
  "scrollbar": {
    "git_diff": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Fleet Dark",
  "toolbar": {
    "quick_actions": false
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 16,
  "vim_mode": true
}

